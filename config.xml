<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.wjfw.wjfwmobile"
        versionCode="10100" 
        version   = "1.01.01">

    <!-- versionCode is optional and Android only -->

    <name>WJFW TV-12</name>

    <description>
        WJFW News Watch 12: News From Where You Live, On The Go!
		Local news, weather and more from wherever you are. Keep up to date with what's going on in the Northwoods, and be ready for whatever Mother Nature brings with our 7 day forecast, weather highlights and radar.
    </description>

    <author href="http://www.byrequestwebdesigns.com" email="zach@brwdmail.com">
        Zach Hext - By Request Web Designs
    </author>
	
	<gap:platform name="ios" />
	<gap:platform name="android" />

	<icon src="Icon.png" />
	
	<!-- iPhone / iPod Touch  -->
	<icon src="ico/Icon-60.png" gap:platform="ios" width="60" height="60" />
	<icon src="ico/Icon-60@2x.png" gap:platform="ios" width="120" height="120" />

	<!-- iPad -->
	<icon src="ico/Icon-76.png" gap:platform="ios" width="76" height="76" />
	<icon src="ico/Icon-76@2x.png" gap:platform="ios" width="152" height="152" />

	<!-- iPhone Spotlight and Settings Icon -->
	<icon src="ico/Icon-Small.png" gap:platform="ios" width="29" height="29" />
	<icon src="ico/Icon-Small@2x.png" gap:platform="ios" width="58" height="58" />

	<!-- Spotlight Icon -->
	<icon src="ico/Icon-40.png" gap:platform="ios" width="40" height="40" />
	<icon src="ico/Icon-40@2x.png" gap:platform="ios" width="80" height="80" />

	<!-- iPhone / iPod Touch -->
	<icon src="ico/Icon.png" gap:platform="ios" width="57" height="57" />
	<icon src="ico/Icon@2x.png" gap:platform="ios" width="114" height="114" />

	<!-- iPad -->
	<icon src="ico/Icon-72.png" gap:platform="ios" width="72" height="72" />
	<icon src="ico/Icon-72@2x.png" gap:platform="ios" width="144" height="144" />

	<!-- iPad Spotlight and Settings Icon -->
	<icon src="ico/Icon-50.png" gap:platform="ios" width="50" height="50" />
	<icon src="ico/Icon-50@2x.png" gap:platform="ios" width="100" height="100" />

	<icon src="ico/ldpi.png" gap:platform="android" gap:qualifier="ldpi" />
	<icon src="ico/mdpi.png" gap:platform="android" gap:qualifier="mdpi" />
	<icon src="ico/hdpi.png" gap:platform="android" gap:qualifier="hdpi" />
	<icon src="ico/xhdpi.png" gap:platform="android" gap:qualifier="xhdpi" />
	<icon src="ico/xxhdpi.png" gap:platform="android" gap:qualifier="xxhdpi" />
	
	
	<gap:splash src="splash.png" />
	
	<!-- iPhone and iPod touch -->
	<gap:splash src="splash/iphone/Default.png" gap:platform="ios" width="320" height="480" />
	<gap:splash src="splash/iphone/Default-Land.png" gap:platform="ios" width="480" height="320" />
	<gap:splash src="splash/iphone/Default@2x.png" gap:platform="ios" width="640" height="960" />
	<gap:splash src="splash/iphone/Default-Land@2x.png" gap:platform="ios" width="960" height="640" />
	<!-- iPhone 5 / iPod Touch (5th Generation) -->
	<gap:splash src="splash/iphone/Default-568h@2x.png" gap:platform="ios" width="640" height="1136" />
	<gap:splash src="splash/iphone/Default-Land568h@2x.png" gap:platform="ios" width="1136" height="640" />
	<!-- iPhone 6  -->
	<gap:splash src="splash/iphone/Default-667h@2x.png" gap:platform="ios" width="750" height="1334" />
	<gap:splash src="splash/iphone/Default-Land667h@2x.png" gap:platform="ios" width="1334" height="750" />
	<!-- iPad -->
	<gap:splash src="splash/iphone/Default-Portrait.png" gap:platform="ios" width="768" height="1024" />
	<gap:splash src="splash/iphone/Default-Landscape.png" gap:platform="ios" width="1024" height="768" />
	<!-- Retina iPad -->
	<gap:splash src="splash/iphone/Default-Portrait@2x.png" gap:platform="ios" width="1536" height="2048" />
	<gap:splash src="splash/iphone/Default-Landscape@2x.png" gap:platform="ios" width="2048" height="1536" />

	<gap:splash src="splash/android/res-notlong-land-ldpi/default.png" gap:platform="android" gap:qualifier="land-ldpi" />
	<gap:splash src="splash/android/res-notlong-land-mdpi/default.png" gap:platform="android" gap:qualifier="land-mdpi" />
	<gap:splash src="splash/android/res-notlong-land-hdpi/default.png" gap:platform="android" gap:qualifier="land-hdpi" />
	<gap:splash src="splash/android/res-notlong-land-xhdpi/default.png" gap:platform="android" gap:qualifier="land-xhdpi" />
	<gap:splash src="splash/android/res-notlong-land-xxhdpi/default.png" gap:platform="android" gap:qualifier="land-xxhdpi" />
	<gap:splash src="splash/android/res-notlong-land-xxxhdpi/default.png" gap:platform="android" gap:qualifier="land-xxxhdpi" />
	<gap:splash src="splash/android/res-notlong-port-ldpi/default.png" gap:platform="android" gap:qualifier="port-ldpi" />
	<gap:splash src="splash/android/res-notlong-port-mdpi/default.png" gap:platform="android" gap:qualifier="port-mdpi" />
	<gap:splash src="splash/android/res-notlong-port-hdpi/default.png" gap:platform="android" gap:qualifier="port-hdpi" />
	<gap:splash src="splash/android/res-notlong-port-xhdpi/default.png" gap:platform="android" gap:qualifier="port-xhdpi" />
	<gap:splash src="splash/android/res-notlong-port-xxhdpi/default.png" gap:platform="android" gap:qualifier="port-xxhdpi" />
	<gap:splash src="splash/android/res-notlong-port-xxxhdpi/default.png" gap:platform="android" gap:qualifier="port-xxxhdpi" />	
	
		
		<!-- New System Plugins -->
	<plugin name="cordova-plugin-whitelist" source="npm" />

		<!--Allow Plugins-->
	<plugin name="com.phonegap.plugin.statusbar" source="pgb" spec="1.1.0" />
	<plugin name="org.apache.cordova.device" source="pgb" spec="0.2.12" />
	<plugin name="org.apache.cordova.geolocation" source="pgb" spec="0.3.10" />
	<plugin name="org.apache.cordova.inappbrowser" source="pgb" spec="0.5.2" />
	<plugin name="org.apache.cordova.network-information" source="pgb" spec="0.2.12" />

		<!--test for video-->
	<!--<gap:plugin name="com.hutchind.cordova.plugins.streamingmedia" version="0.1.3" />-->
	
	
		<!---New System Plugins--->
	<plugin name="cordova-plugin-whitelist" />

	
	<feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" onload="true" />
    </feature>
	
	
	<!-- Preference Options-->
	<preference name="phonegap-version" value="cli-5.2.0" /> 
	<preference name="fullscreen" value="true" />
	<!--iOS-->
	<gap:config-file platform="ios" parent="UIStatusBarHidden">
		<true/>
	</gap:config-file>
	<gap:config-file platform="ios" parent="UIViewControllerBasedStatusBarAppearance">
		<false/>
	</gap:config-file>
	<preference name="target-device" value="universal" /> 
	<preference name="prerendered-icon" value="true" />
	<preference name="detect-data-types" value="false" />
	<preference name="exit-on-suspend" value="false" />
	<!--Android-->
	<preference name="android-minSdkVersion" value="10" />
	<preference name="android-installLocation" value="internalOnly" />
	<preference name="splash-screen-duration" value="3000"/>

	
	<!--WhiteList Access Commands-->
	
		<!-- Allow-Intent for opening links in webbrowser (window.open calls) -->
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" /> <!--allow links to open to phone dialer -->
		<!-- access origin for images,xhdr,etc.-->
	<access origin="*.wjfw.com" />
	<access origin="*.bimvid.com" />
	<access origin="*.bimfs.com" />
	<access origin="*.bimvidlive.com" />
	<access origin="*.field59.com" />
	
	
	<!--<access origin="*" />--> <!-- allow all outgoing requests -->
	
	
</widget>
